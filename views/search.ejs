    <link href="/css/global.css" rel="stylesheet" type="text/css">
    <link href="/css/search.css" rel="stylesheet" type="text/css">
    <!-- JS-->
    <script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
    <script src="https://cdn.firebase.com/js/client/2.3.1/firebase.js"></script>
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
    <script src="//simplewebrtc.com/latest.js"></script>
    <script type="text/javascript" src="https://js.stripe.com/v2/"></script>
    <script type="text/javascript" src="//static.twilio.com/libs/twiliojs/1.2/twilio.min.js"></script>
    <script type="text/javascript" src="js/global.js"></script>
    <script type="text/javascript">
      Stripe.setPublishableKey('pk_live_CmhkakEmZGQeCisMPxX0zGNb');
    </script>
    <title>dibbl</title>
  </head>

  <body>
    <div class="container-fluid">
      <div id="header" class="row">
      </div>

      <div class="title-bar search row">
        <h3>Find the perfect mentor.</h3>
      </div>

      <div class="row search--container">
        <form id="userSearchForm" class="col-md-4 light-gray padded-20">
          <h5 class="inline">Topics of expertise</h5>
          <button class="small blue" id="clearSearchQuery">reset</button>
          <br><br>
          <ul class="topicList topicList-1">
            <li data-selector="music">MUSIC</li>
            <ul class="topicList topicList-2" id="music">
              <li data-selector="performance">PERFORMANCE</li>
              <ul class="topicList topicList-3" id="performance">
                <li class="inactive" data-keyword="percussion">PERCUSSION <span class="xsmall">(coming soon)</span></li>
                <li class="keyword" data-keyword="bass">BASS</li>
                <li class="keyword" data-keyword="guitar">GUITAR</li>
                <li class="keyword" data-keyword="piano">PIANO</li>
                <li class="keyword" data-keyword="vocal">VOCAL</li>
              </ul>
              <li class="keyword" data-keyword="composition">COMPOSITION</li>
              <li class="keyword" data-keyword="music: business">BUSINESS &amp; CAREER</li>
              <li class="keyword" data-keyword="sound engineering">SOUND ENGINEERING</li>
            </ul>
            <li data-selector="visual-art">VISUAL ART</li>
            <ul class="topicList topicList-2" id="visual-art">
              <li class="keyword" data-keyword="fine art">FINE ART</li>
              <li class="keyword" data-keyword="design">DESIGN</li>
            </ul>
            <li class="inactive">WRITING <span class="xsmall">(coming soon)</span></li>
            <li class="inactive">THEATRE <span class="xsmall">(coming soon)</span></li>
            <li class="inactive">FILM <span class="xsmall">(coming soon)</span></li>
            <li class="inactive">CULINARY ARTS <span class="xsmall">(coming soon)</span></li>
          </ul>
          <input type="hidden" id="query">
          <br><br>
        </form>

        <div class="col-md-8">
          <div id="searchResults" class="text-center">
            <img id="noSearchResults" src="../assets/searchresults_default-01.svg" height="600">
          </div>
        </div>
      </div>

      <div id="call-container" class="row">
      </div>

      <div id="footer" class="row">
      </div>
    </div>

    <script>
      var token = '<%= token %>';
    </script>
    <script type="text/javascript" src="js/header.js"></script>
    <script type="text/javascript" src="js/search.js"></script>

    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
