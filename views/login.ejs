<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- FONTS -->
    <link href='https://fonts.googleapis.com/css?family=Montserrat|Roboto:400,300,700' rel='stylesheet' type='text/css'>
    <!-- STYLES -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/normalize/3.0.3/normalize.css" rel="stylesheet" type="text/css">
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <link href="/css/global.css" rel="stylesheet" type="text/css">
    <!-- JS-->
    <script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
    <script src="https://cdn.firebase.com/js/client/2.3.1/firebase.js"></script>
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>

    <title>dibbl</title>

  </head>

  <body>
    <div id="header" class="row">
      <div class="text-left col-xs-6">
        <img src="/assets/dibbl-logo-02.png" class="logo" />
      </div>
      <div class="text-right col-xs-6">
        <i class="fa fa-bell notifications-icon"></i>
        <i class="fa fa-bars menu-icon"></i>
        <div class="notifications dropdown">
          <h5>NOTIFICATIONS:</h5>
          <div id="requests">
          </div>
        </div>
        <div class="menu dropdown">
          <a class="page-nav" data-page="search">find an expert</a><br>
          <a class="page-nav" data-page="profile">my profile</a><br>
          <a class="logout">logout</a>
        </div>
      </div>
    </div>

    <h6>login/signup</h6>
    <div id="home-section-1">
      <img class="splash-logo" src="assets/dibbl-logo-02.png">
      <h6>transforming one-on-one education</h6>

      <div id="login" class="container-fluid">
          <input type="text" id="firstname" placeholder="first name"><br>
          <input type="text" id="lastname" placeholder="last name"><br>
          <input type="email" id="email" placeholder="email"><br>
          <input type="password" id="password" placeholder="password"><br>
          <p class="loginError"></p>
          <button id="userLogin" style="display:none;">Login</button>
          <button id="userSignUp">Sign Up</button>
          <p class="alreadyUser">Already a user? <a>Log in >></a></p>
          <p class="notUserYet" style="display:none;">Not a user yet? <a>Sign up >></a></p>
      </div>
    </div>

    <div id="searchResults">
    </div>






    <script type="text/javascript" src="js/index.js"></script>

    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-68991189-1', 'auto');
      ga('send', 'pageview');

    </script>

  </body>
</html>
